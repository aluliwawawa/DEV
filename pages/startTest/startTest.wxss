.container {
  padding: 20px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 100vh;
}
.question {
  font-size: 18px;
  margin-bottom: 20px;
}
.options {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}
.options button {
  margin: 5px;
  width: 40px;
  height: 40px;
  line-height: 40px;
  text-align: center;
  font-size: 16px;
}
